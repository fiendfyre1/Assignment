#include<stdio.h>

int main()
{
	int salary;
	int day;
	int transport, food, money;

	printf("월급: ");
	scanf("%d", &salary);
	printf("한 달에 출근하는 일 수: ");
	scanf("%d", &day);
	printf("하루 교통비, 식비, 용돈: ");
	scanf("%d %d %d", &transport, &food, &money);
	printf("\n저축액: %d원\n", salary-(transport + food + money)*day);


	return 0;
}
